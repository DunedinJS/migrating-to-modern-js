  "homepage": "https://github.com/DunedinJS/migrating-to-modern-js#readme",
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.9",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.12.1",
    "eslint-config-airbnb": "13.0.0",
